install-dist_gtkdochtmlDATA: $(dist_gtkdochtml_DATA)
	@$(NORMAL_INSTALL)
	@list='$(dist_gtkdochtml_DATA)'; test -n "$(gtkdochtmldir)" || list=; \
	if test -n "$$list"; then \
	  echo " $(MKDIR_P) '$(DESTDIR)$(gtkdochtmldir)'"; \
	  $(MKDIR_P) "$(DESTDIR)$(gtkdochtmldir)" || exit 1; \
	fi; \
	for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  echo "$$d$$p"; \
	done | $(am__base_list) | \
	while read files; do \
	  echo " $(INSTALL_DATA) $$files '$(DESTDIR)$(gtkdochtmldir)'"; \
	  $(INSTALL_DATA) $$files "$(DESTDIR)$(gtkdochtmldir)" || exit $$?; \
	done

uninstall-dist_gtkdochtmlDATA:
	@$(NORMAL_UNINSTALL)
	@list='$(dist_gtkdochtml_DATA)'; test -n "$(gtkdochtmldir)" || list=; \
	files=`for p in $$list; do echo $$p; done | sed -e 's|^.*/||'`; \
	dir='$(DESTDIR)$(gtkdochtmldir)'; $(am__uninstall_files_from_dir)
install-dist_gtkdocimgDATA: $(dist_gtkdocimg_DATA)
	@$(NORMAL_INSTALL)
	@list='$(dist_gtkdocimg_DATA)'; test -n "$(gtkdocimgdir)" || list=; \
	if test -n "$$list"; then \
	  echo " $(MKDIR_P) '$(DESTDIR)$(gtkdocimgdir)'"; \
	  $(MKDIR_P) "$(DESTDIR)$(gtkdocimgdir)" || exit 1; \
	fi; \
	for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  echo "$$d$$p"; \
	done | $(am__base_list) | \
	while read files; do \
	  echo " $(INSTALL_DATA) $$files '$(DESTDIR)$(gtkdocimgdir)'"; \
	  $(INSTALL_DATA) $$files "$(DESTDIR)$(gtkdocimgdir)" || exit $$?; \
	done

uninstall-dist_gtkdocimgDATA:
	@$(NORMAL_UNINSTALL)
	@list='$(dist_gtkdocimg_DATA)'; test -n "$(gtkdocimgdir)" || list=; \
	files=`for p in $$list; do echo $$p; done | sed -e 's|^.*/||'`; \
	dir='$(DESTDIR)$(gtkdocimgdir)'; $(am__uninstall_files_from_dir)
